        <Global>false</Global>
      </ExpressionVariable>
      <ExpressionVariable>
        <Name>evScale</Name>
        <TypeName>Transform</TypeName>
        <ExpressionText>if IsInputEnabled() then if $'lv::_MouseOver' then ScaleAt(1. + 2. / $'lv::_Width', 1. + 2. / $'lv::_Height', $'lv::_Width' / 2., $'lv::_Height' / 2.) else Empty else Empty</ExpressionText>
