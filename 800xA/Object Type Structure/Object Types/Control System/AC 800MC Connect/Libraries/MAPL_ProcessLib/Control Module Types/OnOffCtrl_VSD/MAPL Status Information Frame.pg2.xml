        <ExpressionText>False</ExpressionText>
        <ElementStateVariable>false</ElementStateVariable>
        <Persistent>false</Persistent>
        <Global>false</Global>
      </ExpressionVariable>
      <ExpressionVariable>
        <Name>evLowerRightIndicationLetter</Name>
        <TypeName>String</TypeName>
        <ExpressionText>if !$'pr::OnOffCtrl_VSD:Control Module:vInterlock' then "I" else if $'pr::OnOffCtrl_VSD:Control Module:LIO.OUT.Auto' &amp;&amp; $'pr::OnOffCtrl_VSD:Control Module:FPIntPar.FP_IABlock' &amp;&amp; !$'pr::OnOffCtrl_VSD:Control Module:LIO.IN.IA' || $'pr::OnOffCtrl_VSD:Control Module:FPIntPar.FP_IB1Block' &amp;&amp; !$'pr::OnOffCtrl_VSD:Control Module:LIO.IN.IB1' || $'pr::OnOffCtrl_VSD:Control Module:FPIntPar.FP_IB2Block' &amp;&amp; !$'pr::OnOffCtrl_VSD:Control Module:LIO.IN.IB2' || $'pr::OnOffCtrl_VSD:Control Module:FPIntPar.FP_IB3Block' &amp;&amp; !$'pr::OnOffCtrl_VSD:Control Module:LIO.IN.IB3' || $'pr::OnOffCtrl_VSD:Control Module:FPIntPar.FP_IB4Block' &amp;&amp; !$'pr::OnOffCtrl_VSD:Control Module:LIO.IN.IB4' || $'pr::OnOffCtrl_VSD:Control Module:FPIntPar.FP_PDBlock' &amp;&amp; !$'pr::OnOffCtrl_VSD:Control Module:PreDevInt' then "IB" else if $'pr::OnOffCtrl_VSD:Control Module:LIO.OUT.Auto' &amp;&amp; $'pr::OnOffCtrl_VSD:Control Module:FPIntPar.FP_IABlock' || $'pr::OnOffCtrl_VSD:Control Module:FPIntPar.FP_IB1Block' || $'pr::OnOffCtrl_VSD:Control Module:FPIntPar.FP_IB2Block' || $'pr::OnOffCtrl_VSD:Control Module:FPIntPar.FP_IB3Block' || $'pr::OnOffCtrl_VSD:Control Module:FPIntPar.FP_IB4Block' || $'pr::OnOffCtrl_VSD:Control Module:FPIntPar.FP_PDBlock' then "B" else ""</ExpressionText>
        <ElementStateVariable>false</ElementStateVariable>
        <Persistent>false</Persistent>
        <Global>false</Global>
      </ExpressionVariable>
      <ExpressionVariable>
        <Name>evObjectInstanceName</Name>
        <TypeName>String</TypeName>
        <ExpressionText>$'pr::OnOffCtrl_VSD:Name:Name'</ExpressionText>
        <ElementStateVariable>false</ElementStateVariable>
        <Persistent>false</Persistent>
        <Global>false</Global>
      </ExpressionVariable>
      <ExpressionVariable>
        <Name>evOperatorNoteStatus</Name>
        <TypeName>Boolean</TypeName>
        <ExpressionText>$'pr::OnOffCtrl_VSD:Operator Note:HoldsData'</ExpressionText>
        <ElementStateVariable>false</ElementStateVariable>
        <Persistent>false</Persistent>
        <Global>false</Global>
      </ExpressionVariable>
      <ExpressionVariable>
        <Name>evUpperLeftIndicationBlink</Name>
        <TypeName>Boolean</TypeName>
        <ExpressionText>False</ExpressionText>
        <ElementStateVariable>false</ElementStateVariable>
        <Persistent>false</Persistent>
        <Global>false</Global>
      </ExpressionVariable>
      <ExpressionVariable>
        <Name>evUpperLeftIndicationHighlight</Name>
        <TypeName>Boolean</TypeName>
        <ExpressionText>False</ExpressionText>
        <ElementStateVariable>false</ElementStateVariable>
        <Persistent>false</Persistent>
        <Global>false</Global>
      </ExpressionVariable>
      <ExpressionVariable>
        <Name>evUpperLeftIndicationLetter</Name>
        <TypeName>String</TypeName>
        <ExpressionText>if $'pr::OnOffCtrl_VSD:Control Module:FPIntPar.FP_Forced' then "F" else if $'pr::OnOffCtrl_VSD:Control Module:FPIntPar.FP_Sim' then "S" else ""</ExpressionText>
        <ElementStateVariable>false</ElementStateVariable>
        <Persistent>false</Persistent>
        <Global>false</Global>
      </ExpressionVariable>
      <ExpressionVariable>
        <Name>evUpperRightIndicationBlink</Name>
        <TypeName>Boolean</TypeName>
        <ExpressionText>False</ExpressionText>
        <ElementStateVariable>false</ElementStateVariable>
        <Persistent>false</Persistent>
        <Global>false</Global>
      </ExpressionVariable>
      <ExpressionVariable>
        <Name>evUpperRightIndicationLetter</Name>
        <TypeName>String</TypeName>
        <ExpressionText>if $'pr::OnOffCtrl_VSD:Control Module:BlackoutAct' then "B" else if $'pr::OnOffCtrl_VSD:Control Module:LIO.OUT.Loc' then "L" else if $'pr::OnOffCtrl_VSD:Control Module:LIO.IN.DutyAccepted' then "DU" else if $'pr::OnOffCtrl_VSD:Control Module:LIO.IN.Standby' then "ST" else if $'pr::OnOffCtrl_VSD:Control Module:LIO.OUT.Auto' then "A" else if $'pr::OnOffCtrl_VSD:Control Module:LIO.OUT.Seq' then "S" else if $'pr::OnOffCtrl_VSD:Control Module:LIO.OUT.Cen' then "M" else ""</ExpressionText>
        <ElementStateVariable>false</ElementStateVariable>
        <Persistent>false</Persistent>
        <Global>false</Global>
      </ExpressionVariable>
      <ExpressionVariable>
        <Name>evUpperRightIndicationHighlight</Name>
        <TypeName>Boolean</TypeName>
        <ExpressionText>False</ExpressionText>
        <ElementStateVariable>false</ElementStateVariable>
        <Persistent>false</Persistent>
        <Global>false</Global>
      </ExpressionVariable>
    </ExpressionVariables>
